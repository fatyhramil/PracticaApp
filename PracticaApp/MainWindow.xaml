<Window x:Class="PracticaApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaApp"
        mc:Ignorable="d"
        Title="BeautySaloon" Height="610" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="9*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0">
            <Image Source="/Images/beauty_logo.png" Width="60" Height="60"/>
        </StackPanel>
        <StackPanel Grid.Row="1">
            
            <Frame x:Name="FrameMW" NavigationUIVisibility="Hidden" Height="500"/>
        </StackPanel>

    </Grid>
</Window>
